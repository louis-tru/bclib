[Unit]
Description=dphoto-tx
Documentation=http://gitlab.stars-mine.com/hardware/dphoto-tx
After=network-online.target
Wants=network-online.target

[Service]
User=root
ExecStart=/usr/local/bin/node --inspect=0.0.0.0:9221 /data/dphoto-tx/out/dphoto-tx
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target